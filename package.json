{
  "name": "data-structure-research",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node ./dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "lint": "eslint src/",
    "prettier": "prettier --parser typescript --write \"src/**/*.ts\"",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-cov": "jest --coverage",
    "dev-db-start": "pg-test start && pg-migrations apply --directory src/persistent-tree/seed",
    "dev-db-stop": "pg-test stop"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@databases/pg-test": "^3.1.2",
    "@jest/globals": "^29.3.1",
    "@types/node": "^16.11.65",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.1",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ]
  },
  "dependencies": {
    "@databases/pg": "^5.4.1",
    "@databases/pg-migrations": "^5.0.1",
    "uuid": "^9.0.0"
  }
}
